<div class="container" id="item-detail">
    <div class="row align-items-center">
        <div class="col-md-6">
            <img src="/images/{{bagData.pic}}" alt="{{bagData.name}}">
        </div>
        <div class="col-md-6">
            <h2 class="text-uppercase mb-5">{{bagData.name}}</h2>

            <p>{{bagData.price}}</p>
            <p>{{bagData.made}}</p>
            <p>{{bagData.size}}</p>
            <p>{{bagData.info}}</p>
        </div>
    </div>
</div>


